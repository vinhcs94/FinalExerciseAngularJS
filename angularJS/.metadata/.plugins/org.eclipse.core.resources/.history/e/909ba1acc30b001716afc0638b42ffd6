

app.controller("MainstoreController",function($scope,mainStoreService,$routeParams){

	$scope.getAll = function(){
		mainStoreService.getProducts().then(function(data){
			$scope.products = data;
			console.log ('Get Data Success!!!' + $scope.products)
		})
			
	
	}
	
	$scope.add = function(){
		mainStoreService.getProductIA().then(function(data){
			$scope.products = data;
			console.log ('Get Data Success!!!' + $scope.products)
		})
			
	
	}
	
});
app.directive('myDir',function(){
	return {
		templateUrl: "/fresherangular/views/store/temp.html"
	}
})